<script>
(function() {
    // See https://stackoverflow.com/questions/15661339/how-do-i-fix-blurry-text-in-my-html5-canvas
    var pixel_ratio = function () {
        var ctx = document.createElement("canvas").getContext("2d"),
            dpr = window.devicePixelRatio || 1,
            bsr = ctx.webkitBackingStorePixelRatio ||
                  ctx.mozBackingStorePixelRatio ||
                  ctx.msBackingStorePixelRatio ||
                  ctx.oBackingStorePixelRatio ||
                  ctx.backingStorePixelRatio || 1,
            ratio = dpr / bsr;

        return Math.abs(ratio - 1) < 0.1 ? 1 : ratio;
    };

    var createHiDPICanvas = function(w, h) {
        var can = document.createElement("canvas"),
            ratio = pixel_ratio();
        can.width = w * ratio;
        can.height = h * ratio;
        can.style.width = w + "px";
        can.style.height = h + "px";
        can.getContext("2d").scale(ratio, ratio);
        return can;
    };

    function hideAll() {
        var LINE_HEIGHT = 20,
            FONT = '14px monospace',
            nodes = document.getElementsByClassName('nocopy'),
            lines,
            i = 0,
            j = 0,
            y = 0,
            max_line_length,
            node,
            code,
            width,
            height,
            canvas,
            parentPre,
            ctx,
            existingCanvases,
            n;

        // Delete all existing canvases created by this function
        existingCanvases = document.getElementsByClassName("uoc_nocopy_canvas");
        n = existingCanvases.length;
        for (i = n - 1; i >= 0; i--) {
            canvas = existingCanvases[i];
            canvas.parentNode.removeChild(canvas);
        }

        for (i = 0; i < nodes.length; i++) {
            node = nodes[i];
            if (node.nodeName !== 'CODE') continue;
            max_line_length = 0;
            parentPre = node.parentNode;
            if (parentPre.nodeName !== 'PRE') continue;
            code = node.innerHTML;
            code = code.replace(/<\/?span[^>]*>/g,"");
            code = code.replace(/&gt;/g,">");
            code = code.replace(/&lt;/g,"<");
            code = code.replace(/&amp;/g,"&");
            lines = code.split('\n');

            // First pass - construct a standard-resolution canvas and use its
            // context to measure code. Skip first and last lines
            canvas = document.createElement("CANVAS");
            ctx = canvas.getContext('2d');
            ctx.font = FONT;
            for (j = 1; j < lines.length; j++) {
                max_line_length = Math.max(max_line_length, ctx.measureText(lines[j]).width);
            }

            // Second pass - construct a high DPI-compliant canvas of the
            // required (minimum) size and draw the code
            width = max_line_length;
            height = LINE_HEIGHT * (lines.length - 1) + LINE_HEIGHT / 2;
            canvas = createHiDPICanvas(width, height);
            canvas.id = "canvas" + i;
            canvas.innerHTML = '<pre>' + code + '</pre>';  // fallback content
            wrapper = document.createElement('DIV');
            wrapper.className = 'uoc_nocopy_canvas';
            wrapper.style['padding-left'] = '10px';
            wrapper.appendChild(canvas);
            parentPre.parentNode.insertBefore(wrapper, parentPre.nextSibling);
            canvas.style.backgroundColor = '#f8f8f8';
            wrapper.style.backgroundColor = '#f8f8f8';
            // canvas.style.padding = '5px 20px 10px 20px';
            parentPre.style.display = "none";
            ctx = canvas.getContext('2d');
            ctx.font = FONT;
            y = 0;
            for (j = 1; j < lines.length; j++) {
                y += LINE_HEIGHT;
                ctx.fillText(lines[j], 0, y);
            }
        }
    }
    window.addEventListener("DOMContentLoaded", hideAll);
    window.addEventListener("resize", hideAll);
})();
</script>

